{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/app/*": ["./app/*"],
      "@/styles/*": ["./styles/*"],
      "@/lang/*": ["./lang/*"],
      "@/images/*": ["./public/images/*"],
      "@/appComponents/*": ["./app/components/*"]
    }
  }
}
