{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/public/*": ["./public/*"],
      "@/app/*": ["./app/*"],
      "@/styles/*": ["./styles/*"],
      "@/lang/*": ["./lang/*"],
      "@/images/*": ["./public/images/*"],
      "@/appComponents/*": ["./app/components/*"],
      "@/data/*": ["./app/data/*"],
      "@/store/*": ["./app/store/*"]
    }
  }
}
